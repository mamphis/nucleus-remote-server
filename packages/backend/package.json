{
  "name": "backend",
  "version": "1.0.2",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
    "dev": "concurrently \"npm run watch\" \"nodemon --watch dist dist/index.js\"",
    "build": "tsc",
    "start:prod": "node .",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "docker:build": "docker build . -t docker.home.int/nrs-backend:$(npm pkg get version | sed 's/\"//g')",
    "docker:push": " docker push docker.home.int/nrs-backend:$(npm pkg get version | sed 's/\"//g')",
    "docker:deploy": "npm run patch && npm run docker:build && npm run docker:push",
    "patch": "npm version patch --git-tag-version=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.9",
    "@types/nodemailer": "^6.4.9",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.4",
    "zod": "^3.21.4"
  }
}
